<?xml version="1.0"?>
<document>
	<css>
		<file>view-common.css</file>
	</css>
	<canvas>
		<vbox>
			<id>vbox</id>
			<spacing>10</spacing>
			
			<label>
				<text>this is some funky text {paths:locale}</text>
				<!-- <selectable>true</selectable> -->
			</label>
			<label>
				<id>copy</id>
				<styles>copy copy-override</styles>
				<text>this is some copy text for you</text>
			</label>
			
			<rect>
				<preferred-width>25</preferred-width>
				<preferred-height>25</preferred-height>
				<fill>
					<color>0xff6600</color>
					<alpha>.5</alpha>
				</fill>
				<stroke>
					<thickness>1</thickness>
					<color>0xff0000</color>
				</stroke>
			</rect>
		</vbox>
	</canvas>
</document>