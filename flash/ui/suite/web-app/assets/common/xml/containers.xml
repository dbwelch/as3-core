					<hbox>
						<container>
							<id>box-model-example-target</id>
							<box-model>
								<id>box-model-example</id>
								<target>
									<id>box-model-example-target</id>
								</target>
							</box-model>
						</container>
						<label>
							<id>EXAMPLE-FTE</id>
							<fte>true</fte>
							<styles>note</styles>
							<identifier>container.boxmodel.example</identifier>
							<max-width>220</max-width>
						</label>
					</hbox>
				</vbox>
			</hbox>

			
			<hbox>
				<container>
					<id>outer-container</id>
					<styles>container-border-example outer-container</styles>
