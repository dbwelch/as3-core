					<hbox>
						<container>
							<styles>container-example container-border-example</styles>
						</container>
						<label>
							<id>fte-position</id>
							<fte>true</fte>
							<styles>note</styles>
							<identifier>container.border.example</identifier>
							<max-width>220</max-width>
							<box-model>
								<target>
									<id>fte-position</id>
								</target>
							</box-model>
						</label>			
					</hbox>
			
					<hbox>
						<container>
							<styles>container-example container-border-example container-border-background-example</styles>
						</container>
						<label>
							<fte>true</fte>
							<styles>note</styles>
							<identifier>container.border.background.example</identifier>
							<max-width>220</max-width>
						</label>
					</hbox>

				</vbox>
			
				<vbox>
					<hbox>
						<container>
							<styles>container-example container-border-background-example</styles>
