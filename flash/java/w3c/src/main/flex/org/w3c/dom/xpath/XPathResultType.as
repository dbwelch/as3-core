package org.w3c.dom.xpath
{
	
	/**
	* 	Encapsulates constants for xpath result types.
	*/
	public class XPathResultType extends Object
	{
		/**
		* 	Represents the xpath any type.
		*/
		public static const ANY_TYPE:uint = 0;
		
		/**
		* 	Represents the xpath number type.
		*/
		public static const NUMBER_TYPE:uint = 1;
		
		/**
		* 	Represents the xpath string type.
		*/
		public static const STRING_TYPE:uint = 2;
		
		/**
		* 	Represents the xpath boolean type.
		*/
		public static const BOOLEAN_TYPE:uint = 3;
		
		/**
		* 	Represents the xpath unordered node iterator type.
		*/
		public static const UNORDERED_NODE_ITERATOR_TYPE:uint = 4;
		
		/**
		* 	Represents the xpath ordered node iterator type.
		*/
		public static const ORDERED_NODE_ITERATOR_TYPE:uint = 5;
		
		/**
		* 	Represents the xpath unordered node snapshot type.
		*/
		public static const UNORDERED_NODE_SNAPSHOT_TYPE:uint = 6;
		
		/**
		* 	Represents the xpath ordered node snapshot type.
		*/
		public static const ORDERED_NODE_SNAPSHOT_TYPE:uint = 7;
		
		/**
		* 	Represents the xpath any unordered node type.
		*/
		public static const ANY_UNORDERED_NODE_TYPE:uint = 8;
		
		/**
		* 	Represents the xpath first ordered node type.
		*/
		public static const FIRST_ORDERED_NODE_TYPE:uint = 9;
	}
}