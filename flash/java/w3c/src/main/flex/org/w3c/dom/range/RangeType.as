package org.w3c.dom.range
{
	
	/**
	* 	Encapsulates constants that represent
	* 	types of range.
	*/
	public class RangeType extends Object
	{
		/**
		* 	Represents a start to start range.
		*/
		public static const START_TO_START:uint = 0;
	
		/**
		* 	Represents a start to end range.
		*/
		public static const START_TO_END:uint = 1;
	
		/**
		* 	Represents an end to end range.
		*/
		public static const END_TO_END:uint = 2;				
	
		/**
		* 	Represents an end to start range.
		*/
		public static const END_TO_START:uint = 3;
	}
}