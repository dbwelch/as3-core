package com.ffsys.io.connection {
	
	import com.ffsys.io.loaders.core.ILoaderQueue;
	
	/**
	*	Describes the contract for instances
	*	that can register an <code>ILoaderQueue</code>
	*	with an underlying <code>IConnectionManager</code>.
	*
	*	@langversion ActionScript 3.0
	*	@playerversion Flash 9.0
	*
	*	@author Mischa Williamson
	*	@since  08.12.2007
	*/
	public interface IRegisterLoaderQueue {
		
		/**
		*	Registers an <code>ILoaderQueue</code>
		*	with this instance.
		*	
		*	@param queue The queue to register.
		*/
		function registerLoaderQueue(
			queue:ILoaderQueue = null ):ILoaderQueue;	
	}
}