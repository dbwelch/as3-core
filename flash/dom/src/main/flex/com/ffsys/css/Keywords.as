package com.ffsys.css
{
	
	/**
	* 	Encapsulates constants for css keywords.
	*
	*	@langversion ActionScript 3.0
	*	@playerversion Flash 9.0
	*
	*	@author Mischa Williamson
	*	@since  25.01.2011
	*/
	public class Keywords extends Object
	{
		/**
		* 	Represents the <code>auto</code> keyword.
		*/
		public static const AUTO:String = "auto";
		
		/**
		* 	Represents the <code>normal</code> keyword.
		*/
		public static const NORMAL:String = "normal";
		
		/**
		* 	Represents the <code>inherit</code> keyword.
		*/
		public static const INHERIT:String = "inherit";
		
		/**
		* 	Represents the <code>transparent</code> keyword.
		*/
		public static const TRANSPARENT:String = "transparent";
		
		/**
		* 	Represents a url expression, <code>url()</code>.
		*/
		public static const URL_EXPRESSION:String = "url";
	}
}