package com.ffsys.css
{
	/**
	* 	Encapsulates constants for css bean definitions.
	*
	*	@langversion ActionScript 3.0
	*	@playerversion Flash 9.0
	*
	*	@author Mischa Williamson
	*	@since  24.01.2011
	*/
	public class CssIdentifiers extends Object
	{
		/**
		* 	The identifier for a css namespace.
		*/
		public static const NAMESPACE:String = "namespace";
		
		/**
		* 	The identifier for a css selector.
		*/
		public static const SELECTOR:String = "selector";
		
		/**
		* 	The identifier for a css style rule.
		*/
		public static const STYLE_RULE:String = "style-rule";
		
		/**
		* 	The identifier for a css at &#64; rule.
		*/
		public static const AT_RULE:String = "at-rule";
	}
}