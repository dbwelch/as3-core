package java.util.regex
{
	import java.lang.Exception;

	/**
	* 	Encapsulates an exception thrown when the pattern
	* 	library encounters an error.
	*/
	public class PatternException extends Exception
	{
		/**
		* 	Creates a <code>PatternException</code> instance.
		*/
		public function PatternException()
		{
			super();
		}
	}
}