package org.flashx.ui.display
{
	
	/**
	*	Represents a horizontal graphical rule.
	*
	*	@langversion ActionScript 3.0
	*	@playerversion Flash 9.0
	*
	*	@author Mischa Williamson
	*	@since  03.01.2011
	*/
	public class HorizontalRule extends Divider
	{
		/**
		* 	Creates a <code>HorizontalRule</code> instance.
		*/
		public function HorizontalRule(
			width:Number = NaN,
			height:Number = NaN )
		{
			super( width, height );
		}
	}
}